package New;

public class Hello {
	public static void main(String[] args) {
		
		
		String name="Amol";
		int leng=name.length();
		String rev=" ";
		System.out.println(name);
		System.out.println(len);

		
		
		for (int i=leng-1; i>=0; i--) {
			
			rev=rev+name.charAt(i);
		}
		System.out.print("Reverse String is :: "+rev);
	}

}
